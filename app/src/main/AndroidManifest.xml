<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kellehj1.FYP.birdID">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.birdID">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainMenuActivity"
            android:label="@string/home_title" />

        <activity
            android:name=".FillActivity"
            android:label="@string/fill_tool_title">
        </activity>

        <activity android:name=".ChooseBodyActivity"
            android:label="@string/body_choice_title">
        </activity>

        <activity android:name=".ChooseBeakActivity"
            android:label="@string/beak_choice_title">
        </activity>

        <activity android:name=".BirdEntryActivity" />

        <activity
            android:name=".BirdListActivity"
            android:label="@string/bird_list_title">
        </activity>
    </application>

</manifest>